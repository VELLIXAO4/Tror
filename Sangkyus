    for _, skill in ipairs(MALE_SKILLS) do
        table.insert(menuItems, "ðŸ“– " .. skill.name)
    end
    table.insert(menuItems, "ðŸ”™ Back")
    
    local choice = gg.choice(menuItems, nil, "ðŸ“š MALE SKILLS INFORMATION")
    if choice and choice <= #MALE_SKILLS then
        showSkillInfo(choice)
    end
end

