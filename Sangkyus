function TPP()
    while true do
        local MTPP = gg.multiChoice({
            "ðŸ”™ REFRESH MENU ðŸ”™",
            "ðŸžï¸ DEN OF NO RETURN",
            "â„ï¸ FOREST MANA POOL", 
            "âš”ï¸ PREA PDIP",
            "ðŸ‘¹ OGRE",
            "ðŸ¦ AVES",
            "ðŸ”™ BACK TO DUNGEON ðŸ”™"
        }, nil, "â©â© GUILD MISSION TELEPORT âªâª\nVELLIX_AO Aurcus Online")
        
        if MTPP == nil then
            if gg.isVisible(true) then
    gg.setVisible(false)
                GAMEM()
                break
            end
            gg.sleep(100)
        else
            if MTPP[1] then
                TPP() -- Refresh
            elseif MTPP[2] then
                safeCall(ggtp1)
            elseif MTPP[3] then
                safeCall(ggtp2) 
            elseif MTPP[4] then
                safeCall(ggtp3)
            elseif MTPP[5] then
                safeCall(ggtp4)
            elseif MTPP[6] then
                safeCall(ggtp5)
            elseif MTPP[7] then
                GAMEM()
                break
            end
        end
    end
end

