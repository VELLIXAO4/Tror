-- Device ID Lock
local lockFile = "/sdcard/.gg_device_lock.txt"
local banFile = "/sdcard/.gg_device_banned.txt"

-- Fungsi buat ID unik
local function generateDeviceId()
  math.randomseed(os.time())
  local id = ""
  for i = 1, 16 do
    id = id .. string.char(math.random(65, 90)) -- A-Z
  end
  return id
end

-- Fungsi baca file
local function readFile(path)
  local file = io.open(path, "r")
  if file then
    local content = file:read("*l")
    file:close()
    return content
  end
  return nil
end

-- Fungsi tulis file
local function writeFile(path, content)
  local file = io.open(path, "w")
  if file then
    file:write(content)
    file:close()
    return true
  end
  return false
end

-- Cek status banned
local isBanned = readFile(banFile)
if isBanned == "1" then
gg.setVisible(false)
  gg.alert("üö´ Anda telah dibanned karena gagal verifikasi terlalu banyak.")
  os.exit()
end

-- Cek apakah ID sudah disimpan
local savedId = readFile(lockFile)

if savedId then
  -- Verifikasi: maksimal 3x percobaan
  local attempts = 0
  while attempts < 3 do
    local userId = gg.prompt({"Masukkan ID perangkat untuk menjalankan script:"}, {""})[1]
    if userId == savedId then
    gg.setVisible(false)
      gg.alert("‚úÖ Verifikasi berhasil.")
      break
    else
      attempts = attempts + 1
      gg.setVisible(false)
      gg.alert("‚ùå ID salah! Percobaan ke " .. attempts .. " dari 3.")
    end
  end

  if attempts >= 3 then
    writeFile(banFile, "1")
    gg.setVisible(false)
    gg.alert("üö´ Anda telah dibanned dari script ini.")
    os.exit()
  end

else
  -- Belum pernah dikunci, buat ID baru dan simpan
  local newId = generateDeviceId()
  local success = writeFile(lockFile, newId)
  if success then
    gg.copyText(newId)  -- Salin ke clipboard
    gg.setVisible(false)
    gg.alert("‚úÖ Script berhasil dikunci ke perangkat ini ID kamu: [ " .. newId .. " ] ID telah disalin ke clipboard. Simpan baik-baik ID ini!")
  else
  gg.setVisible(false)
    gg.alert("‚ùå Gagal menyimpan ID perangkat.")
    os.exit()
  end
end

-- Lanjutkan ke script utama
gg.setVisible(false)
gg.alert("‚úÖ Akses diterima. Menjalankan script...")
-- Tambahkan script utama kamu di bawah sini


gg.setVisible(false)
function main()
  local menu = gg.choice({
    "1. Anti Force Close Saat Login",
    "2. Stabilizer Saat Pindah Map",
    "3. Clear Memory Manual",
    "4. Keluar"
  }, nil,os.date("‚è©‚è©SCRIPT FREE VELLIX‚è™‚è™\nüìÜ Date: %A, %B %d %Y\n‚è≤Ô∏è Time: %I:%M %p\nVELLIX_AO Aurcus Online\nüáÆüá© Aurcus 3.2.1 GLOBALüáÆüá©"))

  if menu == 1 then
    antiLogin()
  elseif menu == 2 then
    mapStabilizer()
  elseif menu == 3 then
    clearMemory()
  elseif menu == 4 then
    os.exit()
  end
end

function antiLogin()
  gg.clearResults()
  gg.searchNumber("1337;1;0;0::17", gg.TYPE_DWORD)
  gg.refineNumber("1337")
  local r = gg.getResults(10)
  for i, v in ipairs(r) do
    v.value = "0"
    v.freeze = true
  end
  gg.addListItems(r)
  gg.toast("Anti Force Close Login Aktif")
end

function mapStabilizer()
  gg.setVisible(false)

function disableGGDetection()
  gg.setRanges(gg.REGION_CODE_APP | gg.REGION_JAVA_HEAP)
  gg.searchNumber("4761214;1162690580::17", gg.TYPE_DWORD) -- Nilai acak yang sering muncul dari libgg
  gg.clearResults()
  gg.toast("Bypass Deteksi GG Aktif")
end

function advancedMapStabilizer()
 
  gg.clearResults()
  gg.setRanges(gg.REGION_C_ALLOC)
  gg.searchNumber("20000000~60000000", gg.TYPE_DWORD)
  local res = gg.getResults(20)
  for i, v in ipairs(res) do
    v.value = "0"
    v.freeze = true
  end
  gg.addListItems(res)
  gg.toast("Advanced Stabilizer Aktif")
end

disableGGDetection()
advancedMapStabilizer()
gg.setVisible(false)
gg.clearResults()
gg.setRanges(gg.REGION_C_DATA)
gg.searchNumber("gg", gg.TYPE_BYTE)
gg.clearResults()
gg.clearList()
gg.toast("Mode Hantu Aktif: GG Disembunyikan Total")
gg.toast("Map Crash Protection Siap. Sekarang coba pindah map.")
end

function clearMemory()
  gg.clearResults()
  gg.clearList()
  gg.toast("Memory Game Guardian Dibersihkan")
end

while true do
  if gg.isVisible(true) then
    gg.setVisible(false)
    main()
  end
end
